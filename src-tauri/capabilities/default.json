{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "json"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "clipboard-manager:default",
    "clipboard-manager:allow-read-text",
    "window:allow-set-focus",
    "tray:allow-new",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select"
  ]
}